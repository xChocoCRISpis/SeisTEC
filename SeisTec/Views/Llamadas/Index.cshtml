@model IEnumerable<SeisTec.Models.LlamadasModel>

@{
    ViewData["Title"] = "Llamadas";
}

<h1>Index</h1>

<p>
    @Html.ActionLink("Agregar Teléfono", "AddTelefono", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTelefono)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdTelefono)
                </td>
                <td>
                    @Html.ActionLink("Llamadas", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Agregar Llamada", "Create", new { id = item.IdTelefono }) 

                </td>
            </tr>
        }
    </tbody>
</table>
